{
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "./dist",
    "module": "ESNext",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@ziqian/*":[
        "packages/*/src"
      ]
    },
  },
  "exclude": [
    "**/dist/*"
  ]
}

